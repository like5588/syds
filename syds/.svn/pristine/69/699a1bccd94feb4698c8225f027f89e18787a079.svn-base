package com.ty.photography.model;

import java.util.Date;
/**
 * 用户积分
 * @author wits
 *
 */
public class UserCoin {
	private String id;
	private String userId;
	private Integer totalCoin;	//历史总积分
	private Integer currentCoin;	//当前积分
	private Integer highestCoin;	//历史最高积分
	private Date createTime;
	private Date modifyTime;
	
	private Integer earnCoin;	//获得积分
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public Integer getTotalCoin() {
		return totalCoin;
	}
	public void setTotalCoin(Integer totalCoin) {
		this.totalCoin = totalCoin;
	}
	public Integer getCurrentCoin() {
		return currentCoin;
	}
	public void setCurrentCoin(Integer currentCoin) {
		this.currentCoin = currentCoin;
	}
	public Integer getHighestCoin() {
		return highestCoin;
	}
	public void setHighestCoin(Integer highestCoin) {
		this.highestCoin = highestCoin;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public Date getModifyTime() {
		return modifyTime;
	}
	public void setModifyTime(Date modifyTime) {
		this.modifyTime = modifyTime;
	}
	public Integer getEarnCoin() {
		return earnCoin;
	}
	public void setEarnCoin(Integer earnCoin) {
		this.earnCoin = earnCoin;
	}
	
}

#jsddm
{	width:600px;
	height:54px;
	font-size:18px;
	background:#000;
	color:#fff;
	float:left;}

#jsddm li
	{	float:left;
	text-align:center;
	line-height:54px;}

#jsddm li a
	{	width:120px;
	height:54px;
	display:block;
	color:#fff;
	border:none;
	text-decoration:none;}

#jsddm li a:hover
	{	background:#900;}
		
#jsddm li ul
		{	margin: 0;
			padding: 0;
			position: absolute;
			visibility: hidden;
			width:120px;
			
			}
		
#jsddm li ul li
			{	float: none;
				display: inline}
			
#jsddm li ul li a
			{	width: auto;
				background:url(../mh_images/summarybg.png);
				color:#fff;
				font-size:16px;}
			
#jsddm li ul li a:hover
			{	background: #900}
	

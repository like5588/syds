package com.ty.photography.model;

import java.util.Date;

/**
 * 评委登录信息
 * @author chunxiang
 *
 */

public class JudgeLogon {
	private String id;
	private String account;	//评委账号
	private String password;	//密码
	private String petName;	//名称
	private String judgeGroupId;	//评委组id	
	private Integer judgeRole;		//评委角色 0 组员 1组长
	private Integer type;			//评委类型 9全国,5电信 ,0 审核人员
	private String competitionArea;	//赛区
	private Date createTime;	//创建时间
	private Date modifyTime;	//修改时间
	private Integer status;		//状态	0 有效 1无效
	
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getAccount() {
		return account;
	}
	public void setAccount(String account) {
		this.account = account;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getPetName() {
		return petName;
	}
	public void setPetName(String petName) {
		this.petName = petName;
	}
	public String getJudgeGroupId() {
		return judgeGroupId;
	}
	public void setJudgeGroupId(String judgeGroupId) {
		this.judgeGroupId = judgeGroupId;
	}
	public Integer getJudgeRole() {
		return judgeRole;
	}
	public void setJudgeRole(Integer judgeRole) {
		this.judgeRole = judgeRole;
	}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	public String getCompetitionArea() {
		return competitionArea;
	}
	public void setCompetitionArea(String competitionArea) {
		this.competitionArea = competitionArea;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public Date getModifyTime() {
		return modifyTime;
	}
	public void setModifyTime(Date modifyTime) {
		this.modifyTime = modifyTime;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}

	
}
